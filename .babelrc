{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["ie >=9"]
      },
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    "transform-vue-jsx",
    ["@babel/plugin-transform-runtime", {"corejs": 2}],
    ["@babel/plugin-proposal-decorators", {"legacy": true}]
  ]
}
